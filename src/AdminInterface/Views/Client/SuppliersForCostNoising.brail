<select id="drugstore.NoiseCostExceptSupplier.Id" name="drugstore.NoiseCostExceptSupplier.Id">
	<option value="0" ${'selected' if not IsDefined("FirmCodeOnly")}>Зашумлять все прайс листы всех поставщиков</option>
	<% for supplier in suppliers: %>
	<option value="${supplier.Id}" ${'selected' if IsDefined("FirmCodeOnly") and FirmCodeOnly == supplier.Id}>Кроме [${supplier.Id}].${supplier.Name}</option>
	<% end %>
</select>
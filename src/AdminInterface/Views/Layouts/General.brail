<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<title>
		<% if IsDefined("Title"	): %>
			${Title}
		<% else: %>
			Административный интерфейс
		<% end %>
	</title>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=Edge"> 

	<script type="text/javascript" src="${siteroot}/JavaScript/calendar.js"></script>
	<script type="text/javascript" src="${siteroot}/JavaScript/calendar-ru_win_.js"></script>
	<script type="text/javascript" src="${siteroot}/JavaScript/calendar-setup.js"></script>
	<script type="text/javascript" src="${siteroot}/JavaScript/prototype.js"></script>
	<script type="text/javascript" src="${siteroot}/JavaScript/date-ru-RU.js"></script>
	<script type="text/javascript" src="${siteroot}/JavaScript/Main.js"></script>
	<script type="text/javascript" src="${siteroot}/JavaScript/Search.js"></script>
	<script type="text/javascript" src="${siteroot}/JavaScript/validation.js"></script>
	
	<link href="${siteroot}/Css/calendar-blue.css" type="text/css" rel="stylesheet" />
	<link href="${siteroot}/App_Themes/Main/Main.css" type="text/css" rel="stylesheet" />
	<link href="${siteroot}/Css/Billing.css" type="text/css" rel="stylesheet" />
	<link href="${siteroot}/Css/Table.css" type="text/css" rel="stylesheet" />
	<link href="${siteroot}/Css/Block.css" type="text/css" rel="stylesheet" />
	<link href="${siteroot}/Css/ValidationStyles.css" type="text/css" rel="stylesheet" />
	<link href="${siteroot}/Css/Highlight.css" type="text/css" rel="stylesheet" />
	<link href="${siteroot}/Css/Common.css" type="text/css" rel="stylesheet" />
	<link href="${siteroot}/Css/Contacts.css" type="text/css" rel="stylesheet" />
</head>
<body>
	<% if IsDefined("bredcrumbs"): %>
	<div class="Navigation">
		<ul id="breadcrumb">
			<li>
				<a title="Административный интерфейс" href="${siteroot}/default.aspx">
					<img class="home" src="${siteroot}/images/home.png" alt="Административный интерфейс" />
				</a>
			</li>
			<% for label, url in bredcrumbs: %>
			<li>
				<% if url != "": %>
				<a href="${url}">${label}</a>
				<% else: %>
				${label}
				<% end %>
			</li>
			<% end %>
		</ul>
	</div>
	<% end %>
	${ChildOutput}
</body>
</html>
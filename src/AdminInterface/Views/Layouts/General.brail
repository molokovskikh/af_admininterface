<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=7">
	<title>
		<% if IsDefined("Title"): %>
			${Title}
		<% else: %>
			Административный интерфейс
		<% end %>
	</title>

	${app.CSS("/App_Themes/Main/Main.css")}
	${app.Asset("calendar-blue.css")}
	${app.Asset("Billing.css")}
	${app.Asset("Table.css")}
	${app.Asset("ValidationStyles.css")}
	${app.Asset("Common.css")}
	${app.Asset("Contacts.css")}
	${app.Asset("Highlight.css")}
	${app.Asset("Contacts.css")}
	${app.Asset("Form.css")}

	${app.JS("calendar.js",
		"calendar-ru_win_.js",
		"calendar-setup.js",
		"prototype.js",
		"scriptaculous.js",
		"date-ru-RU.js",
		"Main.js",
		"validation.js")}

</head>
<body>
	<% if IsDefined("bredcrumbs"): %>
	<div class="Navigation">
		<ul id="breadcrumb">
			<li>
				<a title="Административный интерфейс" href="${siteroot}/">
					<img class="home" src="${siteroot}/images/home.png" alt="Административный интерфейс" />
				</a>
			</li>
			<% for label, url in bredcrumbs: %>
			<li>
				<% if url != "": %>
				<a href="${url}">${label}</a>
				<% else: %>
				${label}
				<% end %>
			</li>
			<% end %>
		</ul>
	</div>
	<% end %>
	${ChildOutput}
</body>
</html>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<title>
		<% if IsDefined("Title"	): %>
			${Title}
		<% else: %>
			Административный интерфейс
		<% end %>
	</title>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=Edge"> 
	
	${app.JS("calendar.js",
		"calendar-ru_win_.js",
		"calendar-setup.js",
		"prototype.js",
		"date-ru-RU.js",
		"Main.js",
		"Search.js",
		"validation.js",
		"jquery-1.3.2.min.js")}

	${app.CSS("/App_Themes/Main/Main.css",
		"calendar-blue.css",
		"Billing.css",
		"Table.css",
		"Block.css",
		"ValidationStyles.css",
		"Highlight.css",
		"Common.css",
		"Contacts.css",
		"form.css",
		"User.css")}

	<script>
		jQuery.noConflict();
	</script>
</head>
<body>
	<% if IsDefined("bredcrumbs"): %>
	<div class="Navigation">
		<ul id="breadcrumb">
			<li>
				<a title="Административный интерфейс" href="${siteroot}/default.aspx">
					<img class="home" src="${siteroot}/images/home.png" alt="Административный интерфейс" />
				</a>
			</li>
			<% for label, url in bredcrumbs: %>
			<li>
				<% if url != "": %>
				<a href="${url}">${label}</a>
				<% else: %>
				${label}
				<% end %>
			</li>
			<% end %>
		</ul>
	</div>
	<% end %>
	${ChildOutput}
</body>
</html>